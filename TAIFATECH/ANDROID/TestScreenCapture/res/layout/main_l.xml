<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >

	<RelativeLayout android:id="@+id/RelativeLayout01"
        android:layout_height="fill_parent" 
        android:layout_width="fill_parent">
        <ImageView 
        	android:background="@drawable/background_l"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent" 
            />
        <ImageView 
        	android:background="@drawable/wimo_logo_final"
        	android:layout_marginLeft="0px"
            android:layout_marginTop="0px"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" 
            />
        <ImageView 
        	android:background="@drawable/line_l"
        	android:layout_marginTop="170px"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" 
            />
        <ImageButton android:id="@+id/btn_close"
        	android:background="@drawable/close_button"
            android:layout_marginLeft="735px"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            />
        <ImageButton android:id="@+id/btn_more"
        	android:background="@drawable/more_button"
            android:layout_marginLeft="727px"
            android:layout_marginTop="411px"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="invisible"
            />
        <ImageButton android:id="@+id/btn_radio1"
        	android:background="@drawable/radiobox_check"
            android:layout_marginLeft="222px"
            android:layout_marginTop="84px"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            />
        <ImageButton android:id="@+id/btn_radio2"
        	android:background="@drawable/radiobox_nocheck"
            android:layout_marginLeft="466px"
            android:layout_marginTop="84px"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            />
        <TextView android:id="@+id/tv_title1"
            android:layout_marginLeft="280px"
            android:layout_marginTop="5px"
            android:layout_height="wrap_content" 
            android:layout_width="wrap_content"
            android:textSize="22sp"
            android:text="@string/title1"
            />
        <TextView android:id="@+id/tv_title2"
            android:layout_marginLeft="20px"
            android:layout_marginTop="425px"
            android:layout_height="wrap_content" 
            android:layout_width="wrap_content"
            android:textSize="22sp"
            android:text="@string/title2"
            />
        <TextView android:id="@+id/tv_radio1"
            android:layout_marginLeft="292px"
            android:layout_marginTop="90px"
            android:layout_height="wrap_content" 
            android:layout_width="wrap_content"
            android:textSize="22sp"
            android:text="@string/radio_on"
            />
        <TextView android:id="@+id/tv_radio2"
            android:layout_marginLeft="536px"
            android:layout_marginTop="90px"
            android:layout_height="wrap_content" 
            android:layout_width="wrap_content"
            android:textSize="22sp"
            android:text="@string/radio_off"
            />
    </RelativeLayout>
</LinearLayout>
